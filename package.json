{
    "name": "cpptemplate",
    "displayName": "cpptemplate",
    "description": "Gets automatic template for your new c++ document.",
    "version": "0.0.1",
    "publisher": "Saurav",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.newcpp"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.newcpp",
            "title": "newcpp"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}